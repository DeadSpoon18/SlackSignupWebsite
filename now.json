{
  "version": 2,
  "name": "thatSlack",
  "alias": "thatslack.thatconference.com",
  "env": {
    "AIRTABLE_URL": "@airtable_url",
    "AIRTABLE_TOKEN": "@airtable_token",
    "RECAPTCHA_SITE_KEY": "@recaptchasitekey"
  },
  "builds": [
    { "src": "www/package.json", "use": "@now/next" },
    { "src": "api/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/www/$1" }
  ]
}
